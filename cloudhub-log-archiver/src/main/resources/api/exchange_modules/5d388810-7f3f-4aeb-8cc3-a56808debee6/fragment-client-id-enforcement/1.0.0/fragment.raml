#%RAML 1.0 Trait

#client-id-enforcement
  headers:
    client_id:
      description: このAPIへのアクセスを許可されたクライアント・アプリケーションのClient ID
      type: string
    client_secret:
      description: このAPIへのアクセスを許可されたクライアント・アプリケーションのClient Secret
      type: string
  responses:
    401:
      description: Unauthorized or invalid client application credentials
    500:
      description: Bad response from authorization server